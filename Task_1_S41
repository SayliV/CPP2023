#include <iostream>
#include <clocale>
using namespace std;

float squared_sum(float a, float b) {
    float sum_2 = (a + b)*(a+b);
    return sum_2;
}


int main() {
    setlocale(LC_ALL, "Russian");
    float a, b;
    cout << "Введите чилсо a ";
    cin >> a;
    cout << "Введите чилсо b ";
    cin >> b;
    cout << "Квадрат суммы числа a и b:" << " " << squared_sum(a, b) << endl;
    return 0;
}
