#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    for (int i = 0; i <= 9; i++) // 1 строка 
        cout << i << '\t';
    cout << endl;

    for (int i = 1; i <= 9; i++) 
    {
        cout << i << '\t';
        for (int j = 1; j <= 9; j++) // произведение i*j 
            cout << i * j << '\t';
        cout << endl;
    }
    return 0;
}
